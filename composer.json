    "alipay",
    "gateway",
    "merchant",
    "omnipay",
    "pay",
    "payment",
    "purchase"
  ],
  "homepage": "https://github.com/Knowbox-dev/bud-slide",
  "license": "MIT",
  "authors": [
    {
      "name": "php-cpm",
      "email": "zouyi@knowbox.cn"
    }
  ],
  "require": {
    "phpoffice/phppresentation": "dev-develop",
    "ext-zip": "*",
    "ext-xmlwriter": "*",
    "ext-gd": "*"
